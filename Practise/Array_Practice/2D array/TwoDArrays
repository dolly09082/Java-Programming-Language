import java.util.*;
public class TwoDArrays{
    public static void main (String args[]){
        Scanner sc = new Scanner(System.in);
        int r = sc.nextInt();
        int c = sc.nextInt();
        int [][] a=new int [r][c];
        for(int i=0;i<=1;i++){
            for(int j=0;j<=3;j++){
                System.out.print(a[i][j]);
            }
            System.out.println();
        }
    }
}